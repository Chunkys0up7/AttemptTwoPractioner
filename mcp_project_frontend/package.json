{
  "name": "attempt-two-practioner",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "dev:inspect": "vite --inspect",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "test:ci": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "docs": "typedoc --out docs src",
    "docs:watch": "typedoc --out docs src --watch",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "validate": "npm run lint && npm run type-check && npm run test:ci",
    "clean": "rimraf dist coverage .turbo node_modules",
    "clean:cache": "rimraf .turbo node_modules/.cache",
    "start": "webpack serve --mode development",
    "start:prod": "serve -s dist",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "optimize-deps": "node scripts/optimize-dependencies.js",
    "optimize": "npm run analyze-bundle && npm run optimize-deps"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@google/genai": "^1.1.0",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "esbuild": "^0.25.5",
    "framer-motion": "^10.16.4",
    "idb": "^8.0.3",
    "ioredis": "^5.3.2",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^9.0.2",
    "lightningcss": "1.21.0",
    "lucide-react": "^0.513.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "reactflow": "^11.11.4",
    "serve": "^14.2.1",
    "styled-components": "^6.1.1",
    "@monaco-editor/react": "4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@playwright/test": "^1.52.0",
    "@rollup/rollup-win32-x64-msvc": "^4.41.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/styled-components": "^5.1.34",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.2.2",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.50.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^9.1.7",
    "ioredis-mock": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "mini-css-extract-plugin": "^2.7.6",
    "playwright": "^1.52.0",
    "postcss": "^8.5.4",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.10",
    "rollup-plugin-visualizer": "^6.0.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^4.1.8",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "winston": "^3.17.0",
    "zod": "^3.22.0"
  },
  "description": "A modern web application with optimized performance",
