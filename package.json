{
  "name": "mcp-project-backend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "docs": "typedoc --out docs src",
    "docs:watch": "typedoc --out docs src --watch"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "@google/genai": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vitest": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "typedoc": "^0.25.0"
  },
  "description": "The MCP Backend is a FastAPI-based service for managing machine learning models and workflows.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chunkys0up7/AttemptTwoPractioner.git"
  },
  "keywords": [
    "mcp",
    "machine-learning",
    "workflow",
    "fastapi",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chunkys0up7/AttemptTwoPractioner/issues"
  },
  "homepage": "https://github.com/Chunkys0up7/AttemptTwoPractioner#readme"
}
